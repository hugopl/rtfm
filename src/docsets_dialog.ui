<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <template class="DocsetsDialog" parent="AdwPreferencesWindow">
    <property name="title">Docsets library</property>
    <child>
      <object class="AdwPreferencesPage">
        <property name="title">Available Docsets</property>
        <property name="icon-name">system-software-install-symbolic</property>
            <child>
              <object class="AdwPreferencesGroup" id="available_group">
                <property name="title"></property>
                <property name="description">Docsets are provided by &lt;a href="https://kapeli.com/dash"&gt;Dash&lt;/a&gt;</property>
              </object>
            </child>
      </object>
    </child>
    <child>
      <object class="AdwPreferencesPage">
        <property name="title">Installed Docsets</property>
        <property name="icon-name">view-list-symbolic</property>
        <child>
          <object class="AdwPreferencesGroup" id="installed_group">
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>
